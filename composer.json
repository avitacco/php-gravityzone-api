{
    "name": "avitacco/php-gravityzone-api",
    "description": "A library to interact with the BitDefender GravityZone REST API",
    "type": "library",
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "IndianaUniversity\\GravityZone\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Anthony Vitacco",
            "email": "avitacco@iu.edu"
        }
    ],
    "require": {
        "php": "^8",
        "guzzlehttp/guzzle": "^7.3",
        "datto/json-rpc": "^6.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^0.12.85",
        "phpunit/phpunit": "^9.5",
        "phpmd/phpmd": "^2.10"
    }
}
